import random
n=int(input("Enter the number: "))
z=int(input("Enter the number for the last limit: "))
for i in range(1):
    ran=random.randint(1,20)
    print(ran)
    if (ran>n):
        print("too large")
    elif (ran<n):
        print("too small")
    elif(ran==n):
        print("you guessed it right")
